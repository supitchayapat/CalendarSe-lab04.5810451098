<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="1090.0" stylesheets="@css/db.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.dbshowController">
    <children>
        <TableView fx:id="db" layoutX="11.0" layoutY="14.0" prefHeight="614.0" prefWidth="1068.0">
            <columns>
                <TableColumn prefWidth="431.0" text="Main">
                    <cellValueFactory>
                        <PropertyValueFactory property="date" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn minWidth="6.0" prefWidth="230.0" text="Topic">
                    <cellValueFactory>
                        <PropertyValueFactory property="topic" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn minWidth="6.0" prefWidth="236.0" text="Date">
                    <cellValueFactory>
                        <PropertyValueFactory property="main" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn minWidth="6.0" prefWidth="170.0" text="id">
                    <cellValueFactory>
                        <PropertyValueFactory property="id" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <Button fx:id="Back" layoutX="870.0" layoutY="640.0" mnemonicParsing="false" onAction="#handback" prefHeight="39.0" prefWidth="206.0" text="Back" />
        <TextField fx:id="textSearch"  styleClass="text-input"  layoutX="11.0" layoutY="640.0" prefHeight="39.0" prefWidth="521.0" />
        <Button layoutX="539.0" layoutY="640.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="94.0" text="Search" />
    </children>
</AnchorPane>
